{
    "name": "renderx",
    "version": "1.0.0",
    "description": "On-the-fly HTML Prerender Service using Playwright",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "tsx watch src/index.ts",
        "type-check": "tsc --noEmit",
        "postinstall": "npx playwright install chromium"
    },
    "keywords": [
        "prerender",
        "ssr",
        "playwright",
        "seo"
    ],
    "author": "neilveil",
    "repository": {
        "type": "git",
        "url": "https://github.com/neilveil/renderx.git"
    },
    "homepage": "https://github.com/neilveil/renderx",
    "bugs": {
        "url": "https://github.com/neilveil/renderx/issues"
    },
    "license": "MIT",
    "dependencies": {
        "cheerio": "^1.1.2",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "playwright": "^1.40.0"
    },
    "devDependencies": {
        "@types/cheerio": "^0.22.35",
        "@types/express": "^4.17.21",
        "@types/node": "^20.10.0",
        "tsx": "^4.7.0",
        "typescript": "^5.3.3"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
